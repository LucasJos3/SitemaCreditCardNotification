generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Mailtype{
  orderConfirmation
  paymentConfirmation
}

model Mail{
  id String @id @default(uuid())
  idUser String
  mailDestination String
  mailContent String
  mailType Mailtype
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
}